     # words 'option' or 'flag'.
     if test -n "$lt_prog_compiler_pic -DPIC" && grep "$lt_prog_compiler_pic -DPIC" conftest.err > /dev/null 2>&1 || grep "option" conftest.err > /dev/null 2>&1 || grep "flag" conftest.err > /dev/null 2>&1 ; then
