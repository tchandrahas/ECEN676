module MOL_main

   use REALVEC, only: minimise_BFGS

   use MOL

   implicit none

   saved_self :: MOL*, private

contains

   read_CX_file_name 
   ! Read the CX file name
      stdin.read(.CX_file_name)
   end

   read_CIF_file_name 
   ! Read the CIF file name
      file :: TEXTFILE
      WARN_IF(.CIF_file_name/=" ","a CIF file name already exists")
      WARN_IF(.CIF_data_block_name/=" ","a CIF data block name already exists")
      stdin.read(.CIF_file_name)
      ENSURE(file.exists(name=.CIF_file_name),"no CIF file exists named: "//.CIF_file_name.trim)
   end

   read_CIF_data_block_name 
   ! Read the CIF data block name, set the job name to be this block name.
      ENSURE(.CIF_file_name/=" ","specify CIF file name first")
      stdin.read(.CIF_data_block_name)
      .name = .CIF_file_name.filename_head.trim // "_" &
           // .CIF_data_block_name ! Set name to the crystal data name
      WARN("Job name has now been set to: "//.name.trim)
   end

   find_CIF_crystal_data_block(found) 
   ! Find the *first* block of crystal and atom information from the CIF file,
   ! and set the locator ".CIF_data_block_name".  The name of the molecule (job)
   ! is set to be the name of the cif file concatenated with the data block
   ! name.  
      found :: BIN, optional, OUT
      ENSURE(.CIF_file_name/=" ","no CIF file name")
      .cif.create(.CIF_file_name)
      .find_CIF_crystal_data_block(.cif,found)
      .cif.destroy
   end

   find_CIF_crystal_data_block(cif,found) 
   ! Find the *first* block of crystal and atom information from the "cif" file
   ! (starting from line cif.start_of_data), and set the locator
   ! ".CIF_data_block_name".  The name of the molecule (job) is set to be the
   ! name of the cif file concatenated with the data block name.  
      cif :: CIF
      found :: BIN, optional, OUT
      ENSURE(cif.file.created,"no CIF file name")
      ENSURE(cif.file.name/=" ","no CIF file name")
      if (NOT cif.file.is_open) cif.open
      cif.find_crystal_data_block(found)
      if (present(found)) then
         if (NOT found) return
      end
      .CIF_data_block_name = cif.data_block_name ! <<<<<<<<<
      .name = cif.file.name.filename_head.trim // "_" &
           // cif.data_block_name ! Set name to the crystal data name
      WARN("Job name has now been set to: "//.name.trim)
   end

   process_CIF ::: leaky
   ! Process a CIF file, and try to read crystal and atom information, either
   ! from the current ".CIF_data_block_name", or from the first data block in
   ! the CIF file which is accetable.
      self :: PTR
      if (.CIF_data_block_name==" ") then
         WARN("no CIF data block as specified, looking for first acceptable data block")
         .find_CIF_crystal_data_block
      end
      .process_CIF_data_block
   end

   process_CIF_data_block ::: leaky
   ! Read a named block from the .cif file with name .CIF_file_name and from the
   ! data block with name .CIF_data_block_name, and extract crystal and atom
   ! information. NOTE: .cif is created and destroyed in this routine.
      ENSURE(.CIF_file_name/=" ","no CIF file name")
      ENSURE(.CIF_data_block_name/=" ","no CIF data block name")
      ENSURE(.cif.destroyed,"the cif file is created")
      .cif.create(.CIF_file_name)
      .cif.open
      .cif.find_data_block(.CIF_data_block_name)
      .process_CIF_data_block(.cif) 
      .cif.destroy
   end

   process_CIF_data_block(cif) 
   ! From the current named data block from the "cif", extract crystal and atom
   ! information. 
      cif :: CIF
      ENSURE(cif.data_block_name==.CIF_data_block_name,"inconsistent CIF data block name")
      ENSURE(cif.start_of_data>0,"no start of data in CIF")
      ENSURE(cif.end_of_data>cif.start_of_data,"no end of data in CIF")
      if (NOT cif.file.is_open) cif.open
      .read_crystal_from_CIF(cif)       ! Read crystal info
      .read_atoms_from_CIF(cif)         ! Read in atoms of asymmetric unit
   end

   read_crystal_from_CIF(cif) ::: leaky
   ! Read crystal unitcell and spacegroup information (but NOT atom positions)
   ! from a CIF file called "cif".
      cif :: CIF
      .crystal.destroy
      .crystal.create
      .crystal.read_CIF(cif)
      if (associated(.atom)) then
         .resolve_axis_system
         .crystal.make_reduced_group_data(.atom)
      end
   end

   read_atoms_from_CIF(cif) ::: leaky
   ! Read atom information from a Crystallographic Information File, "cif".
      cif :: CIF
      if (.atom.created) then
         .atom.nullify_basis_part
         .atom.destroy
      end
      .atom.read_CIF(cif)
      .mult = .default_multiplicity
      .set_atom_info
      .resolve_basis_info
      if (associated(.crystal)) then
         .resolve_axis_system
         .crystal.make_reduced_group_data(.atom)
      end
   end

   process_CIF_for_CX ::: leaky
   ! Process and entire .cif file by reading all acceptable data blocks (i.e.
   ! those with crystal and atom information) and then writing this information
   ! to a crystal explorer information file. 
      self :: PTR
      found :: BIN
      cif :: CIF*
      CX_file :: TEXTFILE*
      ENSURE(.CX_file_name/=" ","no CX file name")
      CX_file.create(.CX_file_name)
      CX_file.open(for="write")
      cif.create(.CIF_file_name)
      do
         .find_CIF_crystal_data_block(cif,found)
         if (NOT found) exit            ! No more crystal data blocks
         .process_CIF_data_block(cif)   ! Read new crystal, atom info
         .cluster.create(.crystal,.atom)
         .cluster.radius = ZERO       
         .cluster.add_criteria = "fragment"
         .cluster.start_with_fragment = TRUE
         .cluster.defragment = FALSE
         .cluster.make_info             ! Create the cluster
         .cluster.put
         .put_CX_data(CX_file)          ! Write out the data
         .destroy                       ! Destroy self, create from cif
         .create
         cif.start_of_data = cif.end_of_data  
      end
      cif.destroy
      CX_file.destroy
   end

   put_CX_data
   ! Output information that Crystal Explorer requires to the file 
   ! with name ".CX_file_name".
      CX_file :: TEXTFILE*
   ENSURE(.CX_file_name/=" ","no CX file name")
      CX_file.create(.CX_file_name)
      CX_file.open(for="write")
      .put_CX_data(CX_file)
      CX_file.close
      CX_file.destroy
   end

   put_CX_data(CX_file)
   ! Output information that Crystal Explorer requires to file "CX_file".
   ! NOTE: this requires a CIF file to have been read, and a valid data block
   ! name to be stored in the cif object.
     CX_file :: TEXTFILE*
     save :: TEXTFILE*
     CX_label :: STR
   ENSURE(.cluster.created,"no cluster data")
   ENSURE(.cluster.info_made,"no cluster information")
   ENSURE(.cluster.crystal.created,"no cluster crystal data")
   ENSURE(.cluster.n_fragment_atoms>0,"no fragment size")
   ENSURE(.CIF_data_block_name/=" ","no CIF data block name")
     CX_label = .CIF_data_block_name
     stdout.flush
     stdout.text("Writing Crystal Explorer data file: " // CX_file.name.trim)
     stdout.flush
     save   => stdout                   ! Redirect stdout to out
     stdout => CX_file
     stdout.flush
     stdout.text("Crystal Explorer data")
     stdout.show("begin crystal ",CX_label)
     .cluster.crystal.put_CX(CX_label)  ! Crystal data
     .cluster.put_CX(CX_label)          ! Cluster/fragment positions
     stdout.text("end crystal")
     if (.isosurface.created) then
     if (.isosurface.atom.created) then ! Isosurface information
        .isosurface.put_CX(CX_label)    ! &
                               ! ,.cluster.fragment_atom_indices &
                               ! ,.cluster.nonfragment_atom_indices)  
     end
     end
     stdout.text("Spartan data")
     .cluster.put_spartan(CX_label)     ! Spartan data
     stdout => save                     ! Put back stdout
   end

   make_monomer_MOs ::: leaky
   ! Make the monomer molecular orbital matrix from symmetrically orthonormalised
   ! group (monomer) molecular orbitals. The promolecule orbitals are ordered by group.
   ! The orbitals and the corresponding promolecule density are *not* archived.
   ! This will destroy any existing restricted density matrix and MOs.
      S :: REALMAT*
      .make_group_density(MOs=TRUE,output=FALSE)
      S.create(.n_bf,.n_bf)
      .get_overlap_matrix(S)
      S.change_basis(.molecular_orbitals.restricted)
      stdout.text("S in the monomer basis:")
      stdout.put(S)
      S.destroy
   end

   make_promol_MOs ::: leaky
   ! Make the promolecule molecular orbital matrix from symmetrically orthonormalised
   ! group (monomer) molecular orbitals. The promolecule orbitals are ordered by group.
   ! The orbitals and the corresponding density are *not* archived.
   ! This will destroy any existing restricted density matrix and MOs.
      S :: REALMAT*
      .make_group_density(MOs=TRUE,output=FALSE)
      .symorthonormalise_occupied_MOs
      S.create(.n_bf,.n_bf)
      .get_overlap_matrix(S)
      S.change_basis(.molecular_orbitals.restricted)
      stdout.text("S in the promolecule monomer basis:")
      stdout.put(S)
      S.destroy
   end

