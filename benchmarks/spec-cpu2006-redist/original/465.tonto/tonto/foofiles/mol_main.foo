   main
   ! Main molecule run-loop; assumes stdin and stdout have been defined.
      self :: PTR
      tonto.set_error_output_file(stdout)
      .create
      .put_banner
      .read_keywords   ! do it here
      stdout.flush
      stdout.text(std_time.time_taken("job "//'"'// trim(.name) //'"'))
      stdout.text(std_time.cpu_time_taken("job "//'"'// trim(.name) //'"'))
      .destroy
   end

