program run_cpx

   use TYPES
   use SYSTEM
   use TEXTFILE

   implicit none

   out :: TEXTFILE*
   val :: CPX
   i :: INT

   tonto.initialize

!  out.create("stdin")
!  out.open(for="write")
!  do i = 1,5
!     val = cmplx(i,i+1,kind=CPX_KIND)
!     out.put(val)
!     out.flush
!  end
!  out.close

   stdout.create_stdout
   stdout.open
   stdin.create_stdin
   stdin.open

   do i = 1,5
      stdin.read(val)
      stdout.put(val)
      stdout.flush
   end

end
